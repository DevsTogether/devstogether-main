{
    "extends": ["next", "next/core-web-vitals", "eslint:recommended"],
    "globals": {
        "React": "readonly",
        "JSX": true
    },
    "rules": {
        "no-unused-vars": [
            1,
            { "args": "after-used", "argsIgnorePattern": "^_" }
        ],
        "@next/next/no-img-element": 0,
        "globalReturn": 0
    }
}
