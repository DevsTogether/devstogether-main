{
    "name": "devstogether-main",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "prisma generate && prisma migrate deploy && next build",
        "start": "next start",
        "lint": "next lint",
        "prettier": "prettier --write .",
        "dbml": "npx prisma generate dbml",
        "class-diagram": "tsuml2 --glob \"./!(node_modules/**)/!(*.d|*.spec).ts\" -o ./docs/class-diagram.svg",
        "docs": "npm run class-diagram && npm run dbml && code ./prisma/dbml/schema.dbml && echo '\naccess https://dbdiagram.io/d to generate DER'",
        "precommit": "npm run lint && npm run prettier"
    },
    "dependencies": {
        "@emotion/react": "^11.10.4",
        "@emotion/styled": "^11.10.4",
        "@mui/icons-material": "^5.10.9",
        "@mui/material": "^5.10.9",
        "@mui/styled-engine-sc": "^5.10.6",
        "@next-auth/prisma-adapter": "^1.0.5",
        "@next/bundle-analyzer": "^12.3.1",
        "@prisma/client": "^4.4.0",
        "@sendgrid/mail": "^7.7.0",
        "@supabase/supabase-js": "^2.0.0",
        "next": "12.3.1",
        "next-auth": "^4.13.0",
        "nodemailer": "^6.8.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "socket.io": "^4.5.3",
        "socket.io-client": "^4.5.3",
        "styled-jsx": "^5.0.7",
        "yarn": "^1.22.19"
    },
    "devDependencies": {
        "@types/node": "18.8.5",
        "@types/nodemailer": "^6.4.6",
        "@types/react": "18.0.21",
        "@types/react-dom": "18.0.6",
        "@types/styled-components": "^5.1.26",
        "babel-plugin-styled-components": "^2.0.7",
        "eslint": "8.25.0",
        "eslint-config-next": "12.3.1",
        "prettier": "^2.7.1",
        "prisma": "^4.4.0",
        "prisma-dbml-generator": "^0.9.1",
        "styled-components": "^5.3.6",
        "tsuml2": "^0.6.1",
        "typescript": "4.8.4"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "resolutions": {
        "styled-components": "^5"
    }
}
